#include<iostream>
 using namespace std;
  int main(){
    int n,i,j,temp;
     
    cout<<"Enter The value of n:";
     cin>>n;
     int arr[n];
     cout<<"Enter elements:";
    for(i=0;i<n;i++)
     cin>>arr[i];
    for(i=0;i<n;i++){
      for(j=0;j<n-i-1;j++){
          if(arr[j]>arr[j+1]){
            temp=arr[j];
             arr[j]=arr[j+1];
             arr[j+1]= temp;
          }
        }
    }
    cout<<"Enter sorted elements:";
         for(i=0;i<=n-1;i++)
           cout<<arr[i]<<" ";
    return 0;
  }