#include<iostream>
 using namespace std;
   int main(){
     int i,j,temp,n;
      cout<<"Enter the size:";
       cin>>n;
       int arr[n];
      cout<<"Enter the elements in array";
        for(i=0;i<n;i++)
         cin>>arr[i];
        for(i=1;i<n;i++){
            temp=arr[i];
            j=i-1;
         while(temp <arr[j] &&(j>=0)){
            arr[j+1]=arr[j];
            j--;
         }
         arr[j+1]=temp;
        }
        cout<<"Array after sorting:";
        for(i=0;i<n;i++)
         cout<<arr[i]<<" ";
            
       return 0;
   }